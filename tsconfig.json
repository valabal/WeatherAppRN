{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
    "lib": ["dom"],
    "baseUrl": ".",
    "paths": {
        "@my-bootstrap/*": [
          "./src/bootstrap/*"
        ],        
        "@my-component/*": [
            "./src/component/*"
          ],
        "@my-config/*": [
            "./src/config/*"
         ],
         "@my-module/*": [
            "./src/module/*"
         ],
        "@my-navigation/*": [
            "./src/navigation/*"
         ],
        "@my-util/*": [
            "./src/util/*"
        ]}}
}